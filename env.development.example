# Development Environment Configuration
# Copy to .env for local development

# Environment name
ENVIRONMENT=development

# Logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=DEBUG

# Test interval in seconds (1 = 1-second candles for fast testing, 3600 = 1-hour candles)
# Development uses fast intervals for rapid testing
TEST_INTERVAL=1

# Enable trace logging: Generates detailed CSV files in reports/ folder for strategy auditing
# true = Enable trace logs, false = Disable (default)
ENABLE_TRACE=false

