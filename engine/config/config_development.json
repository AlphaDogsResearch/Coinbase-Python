{
  "default_settings": {
    "module": "builtins",
    "class": "dict",
    "params": {
      "margin_limit": 0.8,
      "selected_symbol": "ETHUSDT",
      "trading_symbols": ["BTCUSDT", "ETHUSDT"],
      "executor_order_type": "@common.interface_order.OrderType.Market",
      "remote_market_data_connection_port": 8080,
      "remote_market_data_connection_name": "Remote Market Data Connection",
      "remote_order_connection_port": 8081,
      "remote_order_connection_name": "Remote Order Connection",
      "database_path": "trading.db"
    }
  },
  "database_manager": {
    "module": "engine.database.database_manager",
    "class": "DatabaseManager",
    "params": {
      "database_path": "@default_settings.database_path",
      "max_connections": 10
    }
  },
  "position": {
    "module": "engine.position.position",
    "class": "Position",
    "params": {
      "symbol": "@default_settings.selected_symbol"
    }
  },
  "risk_manager": {
    "module": "engine.risk.risk_manager",
    "class": "RiskManager",
    "params": {
      "position": "@position"
    }
  },
  "margin_manager": {
    "module": "engine.margin.margin_info_manager",
    "class": "MarginInfoManager"
  },
  "trading_cost_manager": {
    "module": "engine.trading_cost.trading_cost_manager",
    "class": "TradingCostManager"
  },
  "sharpe_calculator": {
    "module": "common.metrics.sharpe_calculator",
    "class": "BinanceFuturesSharpeCalculator"
  },
  "trade_manager": {
    "module": "engine.trades.trades_manager",
    "class": "TradesManager",
    "params": {
      "sharpe_calculator": "@sharpe_calculator",
      "database_manager": "@database_manager"
    }
  },
  "reference_price_manager": {
    "module": "engine.reference_data.reference_price_manager",
    "class": "ReferencePriceManager"
  },
  "position_manager": {
    "module": "engine.position.position_manager",
    "class": "PositionManager",
    "params": {
      "margin_manager": "@margin_manager",
      "trading_cost_manager": "@trading_cost_manager",
      "reference_price_manager": "@reference_price_manager",
      "database_manager": "@database_manager"
    }
  },
  "account": {
    "module": "engine.account.account",
    "class": "Account",
    "params": {
      "margin_limit": "@default_settings.margin_limit"
    }
  },
  "remote_market_data_client": {
    "module": "engine.remote.remote_market_data_client",
    "class": "RemoteMarketDataClient",
    "params": {
      "port": "@default_settings.remote_market_data_connection_port",
      "name": "@default_settings.remote_market_data_connection_name"
    }
  },
  "reference_data_manager": {
    "module": "engine.reference_data.reference_data_manager",
    "class": "ReferenceDataManager",
    "params": {
      "reference_price_manager": "@reference_price_manager"
    }
  },
  "remote_order_client": {
    "module": "engine.remote.remote_order_service_client",
    "class": "RemoteOrderClient",
    "params": {
      "port": "@default_settings.remote_order_connection_port",
      "name": "@default_settings.remote_order_connection_name",
      "margin_manager": "@margin_manager",
      "position_manager": "@position_manager",
      "account": "@account",
      "trading_cost_manager": "@trading_cost_manager",
      "trade_manager": "@trade_manager",
      "reference_data_manager": "@reference_data_manager"
    }
  },
  "executor": {
    "module": "engine.execution.executor",
    "class": "Executor",
    "params": {
      "order_type": "@default_settings.executor_order_type",
      "remote_order_client": "@remote_order_client"
    }
  },
  "order_manager": {
    "module": "engine.management.order_management_system",
    "class": "FCFSOrderManager",
    "params": {
      "executor": "@executor",
      "risk_manager": "@risk_manager",
      "reference_data_manager": "@reference_data_manager",
      "position_manager": "@position_manager",
      "database_manager": "@database_manager"
    }
  },
  "roc_strategy_config": {
    "module": "engine.strategies.roc_mean_reversion_strategy",
    "class": "ROCMeanReversionStrategyConfig",
    "params": {
      "instrument_id": "@default_settings.selected_symbol",
      "bar_type": "ETHUSDT-1h",
      "roc_period": 6,
      "roc_upper": 4,
      "roc_lower": -3,
      "roc_mid": 0,
      "signal_mode": "momentum",
      "exit_mode": "midpoint",
      "notional_amount": 500.0,
      "stop_loss_percent": 0.047,
      "max_holding_bars": 20
    }
  },
  "simple_order_test_strategy_config": {
    "module": "engine.strategies.simple_order_test_strategy",
    "class": "SimpleOrderTestStrategyConfig",
    "params": {
      "instrument_id": "@default_settings.selected_symbol",
      "bar_type": "ETHUSDT-1h",
      "bars_per_trade": 20,
      "notional_amount": 500.0
    }
  },
  "rsi_strategy_config": {
    "module": "engine.strategies.rsi_signal_strategy",
    "class": "RSISignalStrategyConfig",
    "params": {
      "instrument_id": "@default_settings.selected_symbol",
      "bar_type": "ETHUSDT-1h",
      "rsi_period": 30,
      "rsi_upper": 67.0,
      "rsi_lower": 33.0,
      "rsi_mid": 45.0,
      "signal_mode": "momentum",
      "exit_mode": "breakout",
      "notional_amount": 500.0,
      "stop_loss_percent": 0.106,
      "take_profit_percent": 0.05,
      "max_holding_bars": 15,
      "cooldown_bars": 0,
      "use_stop_loss": true,
      "use_take_profit": false,
      "use_max_holding": true,
      "allow_flip": true
    }
  },
  "mom_strategy_config": {
    "module": "engine.strategies.mom_signal_strategy",
    "class": "MOMSignalStrategyConfig",
    "params": {
      "instrument_id": "@default_settings.selected_symbol",
      "bar_type": "ETHUSDT-1h",
      "mom_period": 40,
      "mom_upper": 130.0,
      "mom_lower": -138.0,
      "mom_mid": 27.0,
      "signal_mode": "momentum",
      "exit_mode": "breakout",
      "notional_amount": 500.0,
      "stop_loss_percent": 0.067,
      "take_profit_percent": 0.05,
      "max_holding_bars": 16,
      "cooldown_bars": 0,
      "use_stop_loss": true,
      "use_take_profit": false,
      "use_max_holding": true,
      "allow_flip": true
    }
  },
  "strategy_map": {
    "roc_mean_reversion_strategy": {
      "module": "engine.strategies.roc_mean_reversion_strategy",
      "class": "ROCMeanReversionStrategy",
      "params": {
        "config": "@roc_strategy_config"
      }
    },
    "simple_order_test_strategy": {
      "module": "engine.strategies.simple_order_test_strategy",
      "class": "SimpleOrderTestStrategy",
      "params": {
        "config": "@simple_order_test_strategy_config"
      }
    },
    "rsi_signal_strategy": {
      "module": "engine.strategies.rsi_signal_strategy",
      "class": "RSISignalStrategy",
      "params": {
        "config": "@rsi_strategy_config"
      }
    },
    "mom_signal_strategy": {
      "module": "engine.strategies.mom_signal_strategy",
      "class": "MOMSignalStrategy",
      "params": {
        "config": "@mom_strategy_config"
      }
    }
  }
}
