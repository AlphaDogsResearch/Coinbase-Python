      "remote_order_connection_port": 8081,
      "remote_order_connection_name": "Remote Order Connection",
      "database_path": "trading.db"
    }
  },
  "database_manager": {
    "module": "engine.database.database_manager",
    "class": "DatabaseManager",
    "params": {
      "database_path": "@default_settings.database_path",
      "max_connections": 10
    }
  },
  "position": {
    "module": "engine.position.position",
    "class": "Position",
    "params": {
      "symbol": "@default_settings.selected_symbol"
    }
  },
